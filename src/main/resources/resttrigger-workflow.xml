<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="candidateRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
		<property name="nextNode" ref="endNode_candidateRestValidationWorkflow" />
	</bean>
	<bean id="endNode_candidateRestValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />



	<bean id="clientCorporationRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
		<property name="nextNode" ref="endNode_clientCorporationRestValidationWorkflow" />
	</bean>
	<bean id="endNode_clientCorporationRestValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />



	<bean id="clientContactRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
		<property name="nextNode" ref="endNode_clientContactRestValidationWorkflow" />
	</bean>
	<bean id="endNode_clientContactRestValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />



    <bean id="placementRestValidationWorkFlow" class="com.client.core.base.workflow.node.start.StartNode">
        <property name="nextNode" ref="endNode_placementRestValidationTaskWorkFlow" />
    </bean>
	<bean id="endNode_placementRestValidationTaskWorkFlow" class="com.client.core.base.workflow.node.end.EndNode" />



	<bean id="jobRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
		<property name="nextNode" ref="endNode_jobRestValidationWorkflow" />
	</bean>
	<bean id="endNode_jobRestValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />



    <bean id="opportunityRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
        <property name="nextNode" ref="endNode_candidateRestValidationWorkflow" />
    </bean>
    <bean id="endNode_opportunityValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />



    <bean id="leadRestValidationWorkflow" class="com.client.core.base.workflow.node.start.StartNode">
        <property name="nextNode" ref="endNode_candidateRestValidationWorkflow" />
    </bean>
    <bean id="endNode_leadRestValidationWorkflow" class="com.client.core.base.workflow.node.end.EndNode" />

</beans>
